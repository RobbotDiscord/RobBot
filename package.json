{
  "name": "robbot",
  "private": true,
  "version": "2.0.0",
  "description": "RobBot, a microservice based full stack discord bot utilizing technologies like Redis, MongoDB, RabbitMQ and Docker",
  "scripts": {
    "build": "turbo run build",
    "build:gateway-rb": "turbo run build --scope=gateway-rb",
    "build:config": "turbo run build --scope=config",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prune:gateway-rb": "turbo prune --scope=gateway-rb --docker",
    "start:gateway-rb": "turbo run start --scope=gateway-rb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobbotDiscord/RobBot.git"
  },
  "author": "robigan",
  "license": "GPLv3",
  "workspaces": [
    "instances/*",
    "packages/*",
    "utils/*"
  ],
  "devDependencies": {
    "@types/eslint": "^8",
    "eslint": "^8.19.0",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4"
  },
  "bugs": {
    "url": "https://github.com/RobbotDiscord/RobBot/issues"
  },
  "homepage": "https://github.com/RobbotDiscord/RobBot#readme",
  "packageManager": "yarn@3.2.1"
}
